<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProjectServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.ProjectServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GoalServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.GoalServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TaskServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.TaskServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MyprojectServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.MyprojectServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>EditgoalServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.EditgoalServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>EdittaskServlet</servlet-name>
    <servlet-class>com.kaishengit.pms.web.EdittaskServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowIdea</servlet-name>
    <servlet-class>com.kaishengit.pms.web.ShowIdea</servlet-class>
  </servlet>










  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProjectServlet</servlet-name>
    <url-pattern>/project.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoalServlet</servlet-name>
    <url-pattern>/goal.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TaskServlet</servlet-name>
    <url-pattern>/task.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyprojectServlet</servlet-name>
    <url-pattern>/new_project.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditgoalServlet</servlet-name>
    <url-pattern>/edit_goal.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EdittaskServlet</servlet-name>
    <url-pattern>/edit_task.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowIdea</servlet-name>
    <url-pattern>/showidea.jspx</url-pattern>
  </servlet-mapping>	
  
  <!-- 字符编码过滤器 -->
  <filter>
  	<filter-name>encodingFilter</filter-name>
  	<filter-class>com.kaishengit.pms.web.filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>encodingFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
